\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}OsloMet \uc1\u8211\'96 Mikro\'f8konomi I}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}Av Joachim Th\'f8gersen}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}Oppgave 1}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}Ta utgangspunkt i et marked med fullkommen konkurranse. Anta at tilbudskurven og}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}ettersp\'f8rselskurven er kjente, og gitt ved en hhv. stigende og fallende kurve i et pris-mengde}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}diagram.}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}(a) Forklar begrepene konsumentoverskudd, produsentoverskudd og samfunns\'f8konomisk}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}overskudd.}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}(b) Hvordan vil en pris\'f8kning p\'e5 en substitutt p\'e5virke markedslikevekten og}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}produsentoverskuddet?}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}(c) Hvordan vil en kostnads\'f8kning i bedriftene p\'e5virke markedslikevekten og}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}konsumentoverskuddet?}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}(d) Anta at markedsettersp\'f8rselen er gitt ved:}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}p = 500 \uc1\u8211\'96 0,5XD}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}og at markedstilbudet er gitt ved:}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}p = 200 + 0,25XS}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}Regn ut likevektspris og omsatt kvantum. Vis kurvene i et pris-mengde diagram.}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}(e) Regn ut konsumentoverskudd, produsentoverskudd og samfunns\'f8konomisk}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}overskudd.}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}Oppgave 2}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}Ta utgangspunkt i et marked med fullkommen konkurranse, der markedets ettersp\'f8rsel er}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}kjent ved en fallende ettersp\'f8rselskurve i et pris-mengde diagram.}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}(a) Anta at markedets ettersp\'f8rsel og markedets tilbud er gitt ved henholdsvis:}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}Ettersp\'f8rsel: XD = 800 \uc1\u8211\'96 4p}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}og}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}Tilbud: XS = - 40 + 2p}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}\page Finn ettersp\'f8rsel og tilbud p\'e5 \'abpris-form\'bb. Det vil si at du skal l\'f8se likningene over for}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}p.}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}(b) Vis ettersp\'f8rselskurven og tilbudskurven i et pris-mengde diagram, og regn ut}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}likevektspris og omsatt kvantum.}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}(c) Anta at selgeren blir p\'e5lagt en avgift lik 5 kroner pr. solgte enhet. Ny tilbudsfunksjon}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}blir da p = 25 + 0,5XS. Illustrer denne endringen i figuren du har brukt i oppgave (b).}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}Regn ogs\'e5 ut ny likevektspris og omsatt kvantum.}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}(d) Regn ut d\'f8dvektstapet (effektivitetstapet) som f\'f8lger av avgiften.}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}Oppgave 3}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}Anta at markedets ettersp\'f8rsel etter et bestemt konsumgode er gitt ved:}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}XD = 520 \uc1\u8211\'96 4p}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}der p er prisen p\'e5 godet og X er omsatt kvantum. Markedets tilbudskurve er gitt ved:}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}XS = -20 + 2p}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}Dersom tilbudssiden best\'e5r av monopol, er grensekostnadsfunksjonen gitt ved samme formel.}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}Finn markedslikevekten under fullkommen konkurranse, og sammelign denne med}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}tilpasningen til en profittmaksimerende monopolist. Regn ut effektivitetstapet ved monopol.}{\s28\f0\fs24\lang1044{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s28\itap0{\s28\f0\fs24\lang1044{\*\listtag0}\par}
